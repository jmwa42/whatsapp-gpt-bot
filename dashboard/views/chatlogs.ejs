<h2>ðŸ’¬ Chat Logs</h2>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>User Phone</th>
      <th>Message</th>
      <th>Response</th>
      <th>Time</th>
    </tr>
  </thead>
  <tbody>
    <% logs.forEach(log => { %>
      <tr>
        <td><%= log.userPhone %></td>
        <td><%= log.message %></td>
        <td><%= log.response %></td>
        <td><%= log.createdAt.toLocaleString() %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

