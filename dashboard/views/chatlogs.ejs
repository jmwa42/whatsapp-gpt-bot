
<h2>Chat Logs</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>User</th>
      <th>Message</th>
      <th>Reply</th>
      <th>Time</th>
    </tr>
  </thead>
  <tbody>
    <% chatlogs.forEach(log => { %>
      <tr>
        <td><%= log.user %></td>
        <td><%= log.message %></td>
        <td><%= log.reply %></td>
        <td><%= log.time %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

