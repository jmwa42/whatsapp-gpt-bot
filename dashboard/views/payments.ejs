<table class="table table-bordered">
  <thead><tr><th>Date</th><th>Phone</th><th>Amount</th><th>Status</th></tr></thead>
  <tbody>
    <% payments.forEach(p => { %>
      <tr>
        <td><%= p.date %></td>
        <td><%= p.phone %></td>
